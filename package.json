{
  "name": "angular2-webpack-minimal",
  "version": "0.0.0",
  "description": "An Angular 2 Webpack Starter kit",
  "main": "",
  "scripts": {
    "build": "npm run webpack --colors --display-error-details --display-cached",
    "webpack": "webpack",
    "clean": "rimraf node_modules doc",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean && npm start",
    "watch": "webpack --watch",
    "server": "webpack-dev-server --inline --colors --display-error-details --display-cached --port 3000",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joaogarin/angular2-webpack-minimal.git"
  },
  "author": "joaogarin <github@joaogarin.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joaogarin/angular2-webpack-minimal/issues"
  },
  "homepage": "",
  "dependencies": {
    "angular2": "^2.0.0-alpha.45",
    "reflect-metadata": "0.1.2",
    "zone.js": "0.5.8"
  },
  "devDependencies": {
    "css-loader": "^0.21.0",
    "file-loader": "^0.8.4",
    "object-assign": "4.0.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.4.3",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.6.0",
    "typedoc": "^0.3.12",
    "typescript": "1.6.2",
    "url-loader": "^0.5.6",
    "webpack": "1.12.2",
    "webpack-dev-server": "^1.12.1"
  }
}
